{
  "name": "overzeer",
  "private": true,
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ],
    "catalog": {
      "dotenv": "^17.2.2",
      "zod": "^4.1.13",
      "typescript": "^5",
      "@types/bun": "^1.3.4",
      "elysia": "^1.4.21",
      "@trpc/server": "^11.7.2",
      "better-auth": "^1.4.18",
      "@libsql/client": "0.15.15",
      "libsql": "0.5.22",
      "@trpc/client": "^11.7.2"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "start": "turbo start",
    "check-types": "turbo check-types",
    "dev:native": "turbo -F native dev",
    "dev:web": "turbo -F web dev",
    "dev:server": "turbo -F server dev",
    "db:push": "turbo -F @overzeer/db db:push",
    "db:studio": "turbo -F @overzeer/db db:studio",
    "db:generate": "turbo -F @overzeer/db db:generate",
    "db:migrate": "turbo -F @overzeer/db db:migrate",
    "db:local": "turbo -F @overzeer/db db:local",
    "db:seed": "turbo -F @overzeer/db db:seed",
    "db:reset": "turbo -F @overzeer/db db:reset"
  },
  "dependencies": {
    "@overzeer/env": "workspace:*",
    "dotenv": "^17.3.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@overzeer/config": "workspace:*",
    "@types/bun": "^1.3.9",
    "turbo": "^2.8.9",
    "typescript": "^5.9.3"
  },
  "packageManager": "bun@1.3.8"
}
